{
  "name": "tvNoAds",
  "description": "Tv internet sitelerinde canlı yayın ve vieolardaki reklamı engeller. Adblocktan bağımsız çalışır.",
  "version": "1.0",
  "permissions": [ "webRequest", "webRequestBlocking", "http://*/*", "https://*/*"],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_title": "Video reklamlarını kapat"
  },
  "icons": {
	"128": "icon.png"
  },
  "manifest_version": 2
}